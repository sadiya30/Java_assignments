import java.util.Arrays;
import java.util.Collections;
 
public class SortExample
{
    public static void main(String[] args)
    {
        String arr[] = {"glycine", "leucine","aspartate","glutamate", "glutamine", "arginine","phenylalanine"};
        Arrays.sort(arr);
        System.out.print("Modified arr[] :"+ Arrays.toString(arr));
    }
}
