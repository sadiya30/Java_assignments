public class Avg{

     public static void main(String []args){
         double arr[]={25,277,37,12,1444,155,177, 155, 19,10};
            int sum=0;
            {
            for(int i=0; i<=9; i++)
             {
                sum+=arr[i] ;
             }
            }
            int avg=sum/10;
        System.out.println("the average is:"+sum);
     }
}
